    generated_icu_kits = generated_emergency_kits = generated_pediatrics_kits = empty;
    success[icu] = success[emergency] = success[pediatrics] = empty;
    current_sector = empty;
    kits_to_mount = empty;
    delivery_priority = empty;
    num_tickets = empty;
}</declaration>
